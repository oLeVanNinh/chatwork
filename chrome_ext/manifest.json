{
  "name": "Chatwork Cookie Sync",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["activeTab", "declarativeContent", "storage", "cookies", "http://*/*", "https://*/*", "tabs" ],
  "background": {
    "scripts": ["src/dist/background.js", "src/dist/cookie_sync.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "src/html/popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
